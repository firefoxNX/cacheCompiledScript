<?xml version="1.0"?>
<!DOCTYPE pof-config SYSTEM "pof-config.dtd">

<pof-config>

    <user-type-list>
        <user-type>
            <type-id>1001</type-id>
            <class-name>com.sun.script.javascript.RhinoCompiledScript</class-name>
            <serializer>
                <class-name>com.vineetb.cache.coherence.ReflectionPofSerializer</class-name>
            </serializer>
        </user-type>
        
        <user-type>
            <type-id>2001</type-id>
            <class-name>com.sun.script.javascript.RhinoScriptEngine</class-name>
            <serializer>
                <class-name>com.vineetb.cache.coherence.ReflectionPofSerializer</class-name>
            </serializer>
        </user-type>
		
		<user-type>
            <type-id>3001</type-id>
            <class-name>javax.script.SimpleScriptContext</class-name>
            <serializer>
                <class-name>com.vineetb.cache.coherence.ReflectionPofSerializer</class-name>
            </serializer>
        </user-type>
        
        <user-type>
            <type-id>4001</type-id>
            <class-name>java.io.PrintWriter</class-name>
