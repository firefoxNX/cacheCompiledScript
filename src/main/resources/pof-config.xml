        </user-type>
        
         <user-type>
            <type-id>5001</type-id>
            <class-name>java.io.BufferedWriter</class-name>
            <serializer>
                <class-name>com.vineetb.cache.coherence.ReflectionPofSerializer</class-name>
            </serializer>
        </user-type>
        
         <user-type>
            <type-id>6001</type-id>
            <class-name>java.io.OutputStreamWriter</class-name>
            <serializer>
                <class-name>com.vineetb.cache.coherence.ReflectionPofSerializer</class-name>
            </serializer>
        </user-type>
        
        <user-type>
            <type-id>7001</type-id>
            <class-name>java.io.PrintStream</class-name>
